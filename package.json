    "build": "next build",
    "lint": "eslint .",
    "deploy": "netlify deploy --prod",
    "verify": "node verify-deployment.js",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run type-check",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "lint-strict": "eslint . --ext ts,tsx --max-warnings 0",
